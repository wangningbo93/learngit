*{margin:0px;;padding:0px;font-family:"微软雅黑";}
a{text-decoration:none;}
ul,li,ol,li,dl,dt,dd{list-style-type:none;} 
/*内页推荐样式 webIntro */
#webIntro{width:100%;height:170px;margin:0px auto;background:#f1f1f1;}
#webIntro .webInfo{width:1200px;height:170px;margin:0px auto;}
#webIntro .webInfo .webLeft{width:950px;height:170px;float:left;}
#webIntro .webInfo .webRight{width:220px;height:170px;float:right;}
#webIntro .webInfo .webLeft img.hotImg{float:left;}
#webIntro .webInfo .webLeft .hotList dl{float:right;width:244px;height:115px;background:#fff;margin:18px 10px 0px;
					padding:10px;list-style-type:none;}
#webIntro .webInfo .webLeft .hotList dl{box-shadow: 2px 2px 3px #ddd;}
#webIntro .webInfo .webLeft .hotList dl dt{width:110px;height:100px;background:#ffff33;float:left;}
#webIntro .webInfo .webLeft .hotList dl dd{float:right;font-size:12px;color:#666;width:130px;}
#webIntro .webInfo .webLeft .hotList dl dd.ti{line-height:15px;}
#webIntro .webInfo .webLeft .hotList dl dd.price{padding:15px 0px;}
#webIntro .webInfo .webLeft .hotList dl dd font{color:#f5527a;font-size:18px;}
#webIntro .webInfo .webLeft .hotList dl dd a{display:block;width:77px;height:23px;text-align:center;line-height:23px;
					color:#797979;background:#f5f5f5;border-radius:2px;border: 1px solid #e6e6e6;text-decoration:none;}
#webIntro .webInfo .webRight img.promotion{float:left;}
#webIntro .webInfo .webRight ul{width:165px;height:170px;float:right;padding-top:10px;list-style-type:none;}
#webIntro .webInfo .webRight ul li{font-size:12px;line-height:30px;color:#666;}
#webIntro .webInfo .webRight ul li a{color:#666;}
#webIntro .webInfo .webRight ul li a:hover{color:#f5527a}
/*面包屑导航*/
.webNav{width:1200px;height:55px;margin:0px auto;border-bottom:1px solid #ddd;line-height:55px;font-size:14px;font-family: "微软雅黑";}
.webNav a{color:#666;}
.webNav a:hover{color:#f5527a}
/*礼物综合搜索*/
#giftCom{width:1200px;height:285px;margin:30px auto;border:1px solid #ddd;}
#giftCom h3{height:40px;line-height:40px;font-size:16px;font-weight:100;font-family:"微软雅黑";color:#666;
				border-bottom:1px solid #ddd;background:#f5f5f5;text-indent:10px;}
#giftCom dl{list-style-type:none;width:95%;margin:10px auto;line-height:40px;
			border-bottom:1px solid #ddd;}
#giftCom dl dt{width:10%;float:left;font-weight:bold;color:#666;font-size:14px;}
#giftCom dl dd{width:90%;float:right;font-size:14px;}
#giftCom dl dd a{margin:0px 5px;padding:5px 8px;color:#666;}
#giftCom dl dd a.first{background:#f53e6d;color:#fff;}
#giftCom dl dd a.hover{background:#f53e6d;color:#fff;}
/*商品展示样式*/
#showProduct{width:1200px;margin:30px auto;border:1px solid #ddd;}
#showProduct h3{height:40px;line-height:40px;font-size:16px;font-weight:100;font-family:"微软雅黑";color:#666;border-bottom:1px solid #ddd;background:#f5f5f5;text-indent:10px;}
#showProduct ul{list-style-type:none;}
#showProduct ul li{width:210px;height:310px;border:1px solid #ddd;float:left;margin:10px 14px;box-shadow: 0px 2px 5px #ddd;text-align:center;}
#showProduct ul li h3{background:#fff;border:none;color:#000;height:16px;line-height:16px;}
#showProduct ul li p{width:160px;height:25px;line-height:25px;margin:10px auto;font-size:12px;}
#showProduct ul li p span{width:50%;display: inline-block;height:25px;text-align:center;background:#626262;color:#fff;}
#showProduct ul li p a{width:50%;background:#ed145b;color:#fff;display: inline-block;height:25px;text-align:center;line-height:25px;}
#showProduct ul li p font{color:#f53e6d;}
/*网站底部*/
#webFooter{width:1200px;margin:0px auto;}
#webFooterCon{width:1200px;margin:30px auto;border-top:1px solid #ddd;border-bottom:1px solid #ddd;padding:25px 0px;line-height:16px;font-family:"宋体";font-size:12px;}
#webFooterCon dl{list-style-type:none;width:100px;padding-left:50px;float:left;}
#webFooterCon dl.first{border-right:1px solid #ddd;}
#webFooterCon dl.last{border-left:1px solid #ddd;width:200px;color:#666;font-family:"宋体";}
#webFooterCon dl.last dt{font-size:18px;}
#webFooterCon dl.last dd{font-size:22px;}
#webFooterCon dl dt {height:30px;}
#webFooterCon dl dt a{color:#000;}
#webFooterCon dl dd {height:30px;}
#webFooterCon dl dd a{color:#666;}
p.Copy{font-size:12px; color:#666;text-align:center; line-height:45px;}


/*pruduct详情*/
/*放大镜*/
#webMain{width:1200px;margin:30px auto;height:550px;position:relative;}
#webMain .smallImg{width:400px;height:400px;position:relative;float:left;}
#webMain .bigImg{width:400px;height:400px;position:absolute;left:430px;top:0px;overflow:hidden;display:none;}
#webMain .cover{cursor:crosshair;display:none;width:200px;height:200px;position:absolute;left:0px;top:0px;background:rgba(0,0,0,0.3);filter:alpha(opacity=50);-moz-opacity:0.5;opacity:0.5;}
#webMain .bigImg img{position:absolute;}

#webMain .proInfo{width:700px;height:500px;float:right;}
#webMain .proInfo ul{list-style-type:none;;}
#webMain .proInfo ul li{font-size:14px;height:35px;line-height:35px;font-family:"微软雅黑";padding:5px;}
#webMain .proInfo ul li.first{height:60px;line-height:60px;}
#webMain .proInfo ul li.bor{border-bottom:1px solid #ddd;}
#webMain .proInfo ul li h1{font-size:20px;line-height:60px;}
#webMain .proInfo ul li font{font-size:24px;color:#ff7632;padding-right:20px;}
#webMain .proInfo ul li a.jf{color:#000;float:right;}

#webMain .proInfo ul li input.num{width:50px;height:20px;border:1px solid #ddd;}
#webMain .proInfo ul li.btn{padding-top:20px;}
#webMain .proInfo ul li.btn a{margin-right:20px;}

/*产品推荐*/
#introProduct{width:1200px;height:380px;margin:30px auto;border:1px solid #ddd;}
#introProduct h3{line-height:45px;border-bottom:1px solid #ddd;font-size:18px;color:#f53e6d;text-indent:20px;font-weight:100;margin-bottom: 20px;}
#introProduct ul li{width:220px;float:left;padding:5px;margin:0px 35px;text-align:center;box-shadow:0px 3px 10px #ddd;}
#introProduct ul li a{color:#666;font-size:14px;}
#introProduct ul li p.money{width:220px;font-size:14px;color:#666;margin:10px 0px 20px 0px;;display:block;}
#introProduct ul li p.money font{color:#f53e6d;font-weight:bold;float:left;}
#introProduct ul li p.money span{float:right;}

/*商品描述买家评价家*/
#productDes{width:1200px;margin:0px auto;height:600px;border: 1px solid #e9e9e9;margin-bottom:20px;}
#productDes ul{width:1200px;height:50px;background:#f7f7f7;border-bottom: 1px solid #e9e9e9;cursor:pointer;}
#productDes ul li{width:120px;height:50px;text-align:center;line-height:50px;float:left;font-size:14px;border-right:1px solid #e9e9e9;}
#productDes .ProductSelect{width:1200px;height:500px;margin:40px auto;position:relative;}
#productDes .ProductSelect div{width:1200px;height:400px;position:absolute;left:0px;top:0px;display:none;}
#productDes ul li.active{height:51px;background:#fff;}








